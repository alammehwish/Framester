<http://www.ontologydesignpatterns.org/ont/framester/data/framestersynbase.ttl> rdf:type owl:Ontology ;
 rdfs:comment """The synset frames derived from WordNet synsets (IndivDirectX profile) that accurately match FrameNet framal lexical units,
and can be considered always subsumed under a core Framester frame. Notice that "trope-like" synsets (e.g. 'donor')
are not used to derive new synset frames. This dataset contains punning: synset frames are axiomatized as both individuals and classes (including frame occurrence).""" ;
 owl:versionInfo """1.0, 19-04-2016 by Aldo Gangemi""" ;
 owl:imports <http://www.ontologydesignpatterns.org/ont/framester/framester.owl> ;
 owl:imports <http://www.ontologydesignpatterns.org/ont/framester/data/framestercore.ttl>.

wn30instances:synset-Satan-noun-1
        a       fschema:WnInstance .

wn30instances:synset-Evening_Prayer-noun-1
        a       fschema:WnInstance .

wn30instances:synset-Godhead-noun-1
        a       fschema:WnInstance .

framestercore:Removing
        fschema:occurringWithIndividual
                wn30instances:synset-Bowdler-noun-1 .

wn30instances:synset-Moon-noun-1
        a       fschema:WnInstance .

wn30instances:synset-grail-noun-2
        a       fschema:WnInstance .

wn30instances:synset-Bowdler-noun-1
        a       fschema:WnInstance .

framestercore:Rite  fschema:occurringWithIndividual
                wn30instances:synset-Evening_Prayer-noun-1 .

wn30instances:synset-sun-noun-1
        a       fschema:WnInstance .

wn30instances:synset-Earth-noun-1
        a       fschema:WnInstance .

wn30instances:synset-Trinity-noun-2
        a       fschema:WnInstance .

framestercore:Locale  fschema:occurringWithIndividual
                wn30instances:synset-sun-noun-1 , wn30instances:synset-Moon-noun-1 , wn30instances:synset-Earth-noun-1 .

framestercore:ReligiousBelief
        fschema:occurringWithIndividual
                wn30instances:synset-Trinity-noun-2 , wn30instances:synset-Godhead-noun-1 , wn30instances:synset-Satan-noun-1 , wn30instances:synset-God-noun-1 .

framestercore:Emptying
