        <rdfs:subClassOf rdf:resource="&fschema;ConceptualFrame"/>
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
        <rdfs:subClassOf rdf:resource="&ontolex;LexicalConcept"/>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/framenet/tbox/FrameElement -->

    <owl:Class rdf:about="&fn;FrameElement">
        <rdfs:subClassOf rdf:resource="&fschema;SemanticRole"/>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/framenet/tbox/GenericFE -->

    <owl:Class rdf:about="&fn;GenericFE">
