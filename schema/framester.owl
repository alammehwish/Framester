        <rdfs:domain rdf:resource="&ontolex;LexicalEntry"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="&ontolex;sense"/>
            <rdf:Description rdf:about="&ontolex;isLexicalizedSenseOf"/>
