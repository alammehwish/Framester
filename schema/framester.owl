        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
        <rdfs:domain rdf:resource="&ontolex;LexicalSense"/>
        <rdfs:range rdf:resource="&ontolex;LexicalSense"/>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/lemon/ontolex#sense -->

