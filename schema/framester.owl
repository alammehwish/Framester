<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY bn "http://babelnet.org/rdf/" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY lemon "http://www.lemon-model.net/lemon#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY bn-lemon "http://babelnet.org/model/babelnet#" >
    <!ENTITY ontolex "http://www.w3.org/ns/lemon/ontolex#" >
    <!ENTITY skos "http://www.w3.org/2004/02/skos/core#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY fn "http://www.ontologydesignpatterns.org/ont/framenet/tbox/" >
    <!ENTITY wn30schema "http://www.ontologydesignpatterns.org/ont/wn/wn30/schema/" >
    <!ENTITY semio "http://www.ontologydesignpatterns.org/cp/owl/semiotics.owl#" >
    <!ENTITY own "http://www.ontologydesignpatterns.org/ont/own3/own2dul.owl#" >
    <!ENTITY wn30instances "http://www.ontologydesignpatterns.org/ont/wn/wn30/instances/" >
    <!ENTITY vn31schema "http://www.ontologydesignpatterns.org/ont/vn/vnschema31.owl#" >
    <!ENTITY classif "http://www.ontologydesignpatterns.org/cp/owl/classification.owl#" >
    <!ENTITY special "http://www.ontologydesignpatterns.org/cp/owl/specialization.owl#" >
    <!ENTITY fschema "http://www.ontologydesignpatterns.org/ont/framester/framester.owl#" >
    <!ENTITY dands "http://www.ontologydesignpatterns.org/cp/owl/descriptionandsituation.owl#" >
    <!ENTITY tropes "http://www.ontologydesignpatterns.org/ont/wn/wn30/verbnounsynsettropes.rdf#" >
    <!ENTITY wntroperole "http://www.ontologydesignpatterns.org/ont/wn/wn30/wordnet-verbnountropes.rdf#" >
]>


<rdf:RDF xmlns="http://www.ontologydesignpatterns.org/ont/framester/framester.owl#"
     xml:base="http://www.ontologydesignpatterns.org/ont/framester/framester.owl"
     xmlns:fschema="http://www.ontologydesignpatterns.org/ont/framester/framester.owl#"
     xmlns:tropes="http://www.ontologydesignpatterns.org/ont/wn/wn30/verbnounsynsettropes.rdf#"
     xmlns:wntroperole="http://www.ontologydesignpatterns.org/ont/wn/wn30/wordnet-verbnountropes.rdf#"
     xmlns:dands="http://www.ontologydesignpatterns.org/cp/owl/descriptionandsituation.owl#"
     xmlns:semio="http://www.ontologydesignpatterns.org/cp/owl/semiotics.owl#"
     xmlns:vn31schema="http://www.ontologydesignpatterns.org/ont/vn/vnschema31.owl#"
     xmlns:bn="http://babelnet.org/rdf/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:bn-lemon="http://babelnet.org/model/babelnet#"
     xmlns:wn30instances="http://www.ontologydesignpatterns.org/ont/wn/wn30/instances/"
     xmlns:ontolex="http://www.w3.org/ns/lemon/ontolex#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:classif="http://www.ontologydesignpatterns.org/cp/owl/classification.owl#"
     xmlns:own="http://www.ontologydesignpatterns.org/ont/own3/own2dul.owl#"
     xmlns:special="http://www.ontologydesignpatterns.org/cp/owl/specialization.owl#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:wn30schema="http://www.ontologydesignpatterns.org/ont/wn/wn30/schema/"
     xmlns:fn="http://www.ontologydesignpatterns.org/ont/framenet/tbox/"
     xmlns:lemon="http://www.lemon-model.net/lemon#">
    <owl:Ontology rdf:about="http://www.ontologydesignpatterns.org/ont/framester/framester.owl">
        <rdfs:comment>The schema for Framester data. It also aligns the OWL schemas for WordNet, VerbNet, FrameNet, BabelNet, etc., using the descriptionandsituation.owl and semiotics.owl ontology design patterns. It is also compatible (and aligned) to Lemon and Ontolex-Lemon.</rdfs:comment>
        <owl:versionInfo>1.0, 19-04-2016 by Aldo Gangemi</owl:versionInfo>
        <owl:imports rdf:resource="http://babelnet.org/rdf/"/>
        <owl:imports rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/descriptionandsituation.owl"/>
        <owl:imports rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/semiotics.owl"/>
        <owl:imports rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/specialization.owl"/>
        <owl:imports rdf:resource="&fn;schema.owl"/>
        <owl:imports rdf:resource="http://www.ontologydesignpatterns.org/ont/own3/own2dul.owl"/>
        <owl:imports rdf:resource="http://www.ontologydesignpatterns.org/ont/vn/vnschema31.owl"/>
        <owl:imports rdf:resource="http://www.ontologydesignpatterns.org/ont/wn/wn30/schema/"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.ontologydesignpatterns.org/ont/framester/framester.owl#framenetDef -->

    <owl:AnnotationProperty rdf:about="&fschema;framenetDef"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.lemon-model.net/lemon#isReferenceOf -->

    <owl:ObjectProperty rdf:about="&lemon;isReferenceOf">
        <owl:equivalentProperty rdf:resource="&ontolex;isReferenceOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/description.owl#usesConcept -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/cp/owl/description.owl#usesConcept"/>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/semiotics.owl#denotes -->

    <owl:ObjectProperty rdf:about="&semio;denotes"/>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/semiotics.owl#expresses -->

    <owl:ObjectProperty rdf:about="&semio;expresses"/>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/semiotics.owl#hasConceptualization -->

    <owl:ObjectProperty rdf:about="&semio;hasConceptualization">
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/semiotics.owl#isConceptualizationOf -->

    <owl:ObjectProperty rdf:about="&semio;isConceptualizationOf">
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/semiotics.owl#relatedMeaning -->

    <owl:ObjectProperty rdf:about="&semio;relatedMeaning">
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/specialization.owl#requires -->

    <owl:ObjectProperty rdf:about="&special;requires">
        <rdfs:subPropertyOf rdf:resource="&semio;relatedMeaning"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/specialization.owl#specializes -->

    <owl:ObjectProperty rdf:about="&special;specializes">
        <rdfs:subPropertyOf rdf:resource="&semio;relatedMeaning"/>
        <rdfs:subPropertyOf rdf:resource="&fschema;subsumedUnder"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/framenet/tbox/framalLexUnitOf -->

    <owl:ObjectProperty rdf:about="&fn;framalLexUnitOf">
        <rdfs:subPropertyOf rdf:resource="&semio;relatedMeaning"/>
        <rdfs:subPropertyOf rdf:resource="&fschema;subsumedUnder"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/framenet/tbox/hasFERelation -->

    <owl:ObjectProperty rdf:about="&fn;hasFERelation">
        <rdfs:subPropertyOf rdf:resource="&semio;relatedMeaning"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/framenet/tbox/hasFrameElement -->

    <owl:ObjectProperty rdf:about="&fn;hasFrameElement">
        <rdfs:subPropertyOf rdf:resource="&semio;relatedMeaning"/>
        <rdfs:subPropertyOf rdf:resource="&fschema;semanticRole"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/framenet/tbox/hasFrameRelation -->

    <owl:ObjectProperty rdf:about="&fn;hasFrameRelation">
        <rdfs:subPropertyOf rdf:resource="&semio;relatedMeaning"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/framenet/tbox/hasMember -->

    <owl:ObjectProperty rdf:about="&fn;hasMember">
        <rdfs:subPropertyOf rdf:resource="&semio;relatedMeaning"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/framenet/tbox/hasSemType -->

    <owl:ObjectProperty rdf:about="&fn;hasSemType">
        <rdfs:subPropertyOf rdf:resource="&semio;relatedMeaning"/>
        <rdfs:subPropertyOf rdf:resource="&fschema;ontoType"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/framenet/tbox/hasSubType -->

    <owl:ObjectProperty rdf:about="&fn;hasSubType">
        <rdfs:subPropertyOf rdf:resource="&fschema;subsumes"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/framenet/tbox/hasSuperFE -->

    <owl:ObjectProperty rdf:about="&fn;hasSuperFE">
        <rdfs:subPropertyOf rdf:resource="&fschema;subsumedUnder"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/framenet/tbox/hasSuperType -->

    <owl:ObjectProperty rdf:about="&fn;hasSuperType">
        <rdfs:subPropertyOf rdf:resource="&special;specializes"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/framenet/tbox/inheritsFrom -->

    <owl:ObjectProperty rdf:about="&fn;inheritsFrom">
        <rdfs:subPropertyOf rdf:resource="&fschema;subsumedUnder"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/framenet/tbox/isInheritedBy -->

    <owl:ObjectProperty rdf:about="&fn;isInheritedBy">
        <rdfs:subPropertyOf rdf:resource="&fschema;subsumes"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/framester/framester.owl#binaryProjection -->

    <owl:ObjectProperty rdf:about="&fschema;binaryProjection">
        <rdfs:subPropertyOf rdf:resource="&fschema;hasProjection"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/framester/framester.owl#co-participation -->

    <owl:ObjectProperty rdf:about="&fschema;co-participation">
        <rdfs:subPropertyOf rdf:resource="&fschema;binaryProjection"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/framester/framester.owl#coreRole -->

    <owl:ObjectProperty rdf:about="&fschema;coreRole">
        <rdfs:subPropertyOf rdf:resource="&fschema;semanticRole"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/framester/framester.owl#externalRole -->

    <owl:ObjectProperty rdf:about="&fschema;externalRole">
        <rdfs:subPropertyOf rdf:resource="&fschema;semanticRole"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/framester/framester.owl#hasProjection -->

    <owl:ObjectProperty rdf:about="&fschema;hasProjection">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/description.owl#usesConcept"/>
        <rdfs:domain rdf:resource="&fn;Frame"/>
        <rdfs:range rdf:resource="&fschema;FrameProjection"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/framester/framester.owl#necessaryRole -->

    <owl:ObjectProperty rdf:about="&fschema;necessaryRole">
        <rdfs:subPropertyOf rdf:resource="&fschema;semanticRole"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/framester/framester.owl#occurrenceOf -->

    <owl:ObjectProperty rdf:about="&fschema;occurrenceOf">
        <rdfs:range rdf:resource="&fschema;Frame"/>
        <rdfs:domain rdf:resource="&fschema;FrameOccurrence"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/framester/framester.owl#ontoType -->

    <owl:ObjectProperty rdf:about="&fschema;ontoType">
        <rdfs:comment>The relation between unary projections of a frame, and their ontological type.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&semio;relatedMeaning"/>
        <rdfs:range rdf:resource="&fschema;OntologyClass"/>
        <rdfs:domain rdf:resource="&fschema;UnaryProjection"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/framester/framester.owl#optionalRole -->

    <owl:ObjectProperty rdf:about="&fschema;optionalRole">
        <rdfs:subPropertyOf rdf:resource="&fschema;semanticRole"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/framester/framester.owl#projectionOf -->

    <owl:ObjectProperty rdf:about="&fschema;projectionOf">
        <rdfs:range rdf:resource="&fschema;Frame"/>
        <rdfs:domain rdf:resource="&fschema;FrameProjection"/>
        <owl:inverseOf rdf:resource="&fschema;hasProjection"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/framester/framester.owl#semanticRole -->

    <owl:ObjectProperty rdf:about="&fschema;semanticRole">
        <rdfs:domain rdf:resource="&fschema;Frame"/>
        <rdfs:range rdf:resource="&fschema;SemanticRole"/>
        <rdfs:subPropertyOf rdf:resource="&fschema;binaryProjection"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/framester/framester.owl#subsumedUnder -->

    <owl:ObjectProperty rdf:about="&fschema;subsumedUnder">
        <rdfs:range rdf:resource="&semio;Meaning"/>
        <rdfs:domain rdf:resource="&semio;Meaning"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/framester/framester.owl#subsumes -->

    <owl:ObjectProperty rdf:about="&fschema;subsumes">
        <rdfs:range rdf:resource="&semio;Meaning"/>
        <rdfs:domain rdf:resource="&semio;Meaning"/>
        <rdfs:subPropertyOf rdf:resource="&semio;relatedMeaning"/>
        <owl:inverseOf rdf:resource="&fschema;subsumedUnder"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/framester/framester.owl#unaryProjection -->

    <owl:ObjectProperty rdf:about="&fschema;unaryProjection">
        <rdfs:subPropertyOf rdf:resource="&fschema;hasProjection"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/framester/framester.owl#vnRole -->

    <owl:ObjectProperty rdf:about="&fschema;vnRole">
        <rdfs:subPropertyOf rdf:resource="&fschema;semanticRole"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/own3/own2dul.owl#d0 -->

    <owl:ObjectProperty rdf:about="&own;d0">
        <rdfs:range rdf:resource="&fschema;OntologyClass"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
        <rdfs:subPropertyOf rdf:resource="&ontolex;reference"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/own3/own2dul.owl#dul -->

    <owl:ObjectProperty rdf:about="&own;dul">
        <rdfs:range rdf:resource="&fschema;OntologyClass"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
        <rdfs:subPropertyOf rdf:resource="&ontolex;reference"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/own3/own2dul.owl#hyp -->

    <owl:ObjectProperty rdf:about="&own;hyp">
        <rdfs:subPropertyOf rdf:resource="&special;specializes"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/own3/own2dul.owl#ihyp -->

    <owl:ObjectProperty rdf:about="&own;ihyp">
        <rdfs:subPropertyOf rdf:resource="&fschema;subsumedUnder"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/own3/own2dul.owl#proxhyp -->

    <owl:ObjectProperty rdf:about="&own;proxhyp">
        <rdfs:subPropertyOf rdf:resource="&special;specializes"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/vn/vnschema31.owl#containsVerbSense -->

    <owl:ObjectProperty rdf:about="&vn31schema;containsVerbSense">
        <rdfs:subPropertyOf rdf:resource="&semio;relatedMeaning"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
        <rdfs:subPropertyOf rdf:resource="&ontolex;lexicalizedSense"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/vn/vnschema31.owl#hasArgument -->

    <owl:ObjectProperty rdf:about="&vn31schema;hasArgument">
        <rdfs:subPropertyOf rdf:resource="&semio;relatedMeaning"/>
        <rdfs:subPropertyOf rdf:resource="&fschema;semanticRole"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/vn/vnschema31.owl#hasPredicate -->

    <owl:ObjectProperty rdf:about="&vn31schema;hasPredicate">
        <rdfs:subPropertyOf rdf:resource="&semio;relatedMeaning"/>
        <rdfs:subPropertyOf rdf:resource="&fschema;hasProjection"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/vn/vnschema31.owl#hasSelectionalRestriction -->

    <owl:ObjectProperty rdf:about="&vn31schema;hasSelectionalRestriction">
        <rdfs:subPropertyOf rdf:resource="&semio;relatedMeaning"/>
        <rdfs:subPropertyOf rdf:resource="&fschema;unaryProjection"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/wn/wn30/schema/adjectivePertainsTo -->

    <owl:ObjectProperty rdf:about="&wn30schema;adjectivePertainsTo">
        <rdfs:subPropertyOf rdf:resource="&semio;relatedMeaning"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/wn/wn30/schema/adverbPertainsTo -->

    <owl:ObjectProperty rdf:about="&wn30schema;adverbPertainsTo">
        <rdfs:subPropertyOf rdf:resource="&semio;relatedMeaning"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/wn/wn30/schema/antonymOf -->

    <owl:ObjectProperty rdf:about="&wn30schema;antonymOf">
        <rdfs:subPropertyOf rdf:resource="&semio;relatedMeaning"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/wn/wn30/schema/attribute -->

    <owl:ObjectProperty rdf:about="&wn30schema;attribute">
        <rdfs:subPropertyOf rdf:resource="&semio;relatedMeaning"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/wn/wn30/schema/causes -->

    <owl:ObjectProperty rdf:about="&wn30schema;causes">
        <rdfs:subPropertyOf rdf:resource="&semio;relatedMeaning"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/wn/wn30/schema/classifies -->

    <owl:ObjectProperty rdf:about="&wn30schema;classifies">
        <rdfs:subPropertyOf rdf:resource="&semio;relatedMeaning"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/wn/wn30/schema/containsWordSense -->

    <owl:ObjectProperty rdf:about="&wn30schema;containsWordSense">
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
        <rdfs:subPropertyOf rdf:resource="&ontolex;lexicalizedSense"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/wn/wn30/schema/derivationallyRelated -->

    <owl:ObjectProperty rdf:about="&wn30schema;derivationallyRelated">
        <rdfs:subPropertyOf rdf:resource="&semio;relatedMeaning"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/wn/wn30/schema/entails -->

    <owl:ObjectProperty rdf:about="&wn30schema;entails">
        <rdfs:subPropertyOf rdf:resource="&semio;relatedMeaning"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/wn/wn30/schema/holonymOf -->

    <owl:ObjectProperty rdf:about="&wn30schema;holonymOf">
        <rdfs:subPropertyOf rdf:resource="&semio;relatedMeaning"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/wn/wn30/schema/hyponymOf -->

    <owl:ObjectProperty rdf:about="&wn30schema;hyponymOf">
        <rdfs:subPropertyOf rdf:resource="&special;specializes"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/wn/wn30/schema/instanceOf -->

    <owl:ObjectProperty rdf:about="&wn30schema;instanceOf">
        <rdfs:subPropertyOf rdf:resource="&semio;hasConceptualization"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/wn/wn30/schema/meronymOf -->

    <owl:ObjectProperty rdf:about="&wn30schema;meronymOf">
        <rdfs:subPropertyOf rdf:resource="&semio;relatedMeaning"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/wn/wn30/schema/participle -->

    <owl:ObjectProperty rdf:about="&wn30schema;participle">
        <rdfs:subPropertyOf rdf:resource="&semio;relatedMeaning"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/wn/wn30/schema/sameVerbGroupAs -->

    <owl:ObjectProperty rdf:about="&wn30schema;sameVerbGroupAs">
        <rdfs:subPropertyOf rdf:resource="&semio;relatedMeaning"/>
