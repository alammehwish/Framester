    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/framenet/tbox/LexUnit -->

    <owl:Class rdf:about="&fn;LexUnit">
        <rdfs:subClassOf rdf:resource="&semio;InformationEntity"/>
